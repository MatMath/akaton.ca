{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "prebuild": "npm run build:authors",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "build:authors": "./replace-config.sh {{AUTHOR_MATMATH}} ${AUTHOR_MATMATH}; ./replace-config.sh {{AUTHOR_CESINE}} ${AUTHOR_CESINE}",
    "build:authors:undo": "./replace-config.sh ${AUTHOR_MATMATH} {{AUTHOR_MATMATH}} ; ./replace-config.sh ${AUTHOR_CESINE} {{AUTHOR_CESINE}}",
    "server": "hexo server",
    "preserver": "npm run build:authors",
    "postserver": "npm run build:authors:undo"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "hexo": "^6.3.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-renderer-stylus": "^3.0.0",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0"
  }
}